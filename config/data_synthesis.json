{
    "enable_draw": false,
    "scene_type": "data_synthesis",
    "default_config_path": "config/sim_configs/se_config.json",
    "pause_at_first": false,
    "default_timestep": 1e-3,
    "window_height": 600,
    "window_width": 800,
    "camera": {
        "camera_pos": [
            0.5,
            0.5,
            0.5
        ],
        "camera_focus": [
            0.0,
            0.15,
            0
        ],
        "fov": 45,
        "near": 0.01,
        "far": 100
    },
    "property_manager": {
        "properties": {
            "stretch_warp": {
                "range": [
                    1e5,
                    1e5
                ],
                "num_of_samples": 1,
                "visible": false
            },
            "stretch_weft": {
                "range": [
                    1e5,
                    1e5
                ],
                "num_of_samples": 1,
                "visible": false
            },
            "bending_warp": {
                "range": [
                    5e2,
                    5e3
                ],
                "num_of_samples": 1,
                "visible": true,
                "index": 0
            },
            "bending_weft": {
                "range": [
                    5e2,
                    5e3
                ],
                "num_of_samples": 1,
                "visible": true,
                "index": 1
            },
            "bending_bias": {
                "range": [
                    1e3,
                    2e3
                ],
                "num_of_samples": 200,
                "visible": true,
                "index": 2
            }
        },
        "mode": "linear",
        "exchangeable_pairs": [
            [
                "bending_warp",
                "bending_weft"
            ],
            [
                "stretch_warp",
                "stretch_weft"
            ]
        ]
    },
    "enable_noise": true,
    "noise": {
        "noised_samples": 3,
        "enable_low_freq_noise": true,
        "max_fold_amp": 0.05,
        "min_fold_num": 3,
        "max_fold_num": 3
    },
    "convergence_threshold": 0.001,
    "enable_data_cleaner": true,
    "data_cleaner_threshold": 0.005,
    "enable_collision_detection": false
}