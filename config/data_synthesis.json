{
    "enable_draw": false,
    "scene_type": "data_synthesis",
    "default_config_path": "config/sim_configs/se_config.json",
    "enable_data_aug": false,
    "pause_at_first": true,
    "convergence_threshold": 0.01,
    "window_height": 600,
    "window_width": 800,
    "camera": {
        "camera_pos": [
            0.5,
            0.5,
            0.5
