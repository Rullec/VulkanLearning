{
    "name": "CNNParamNet",
    "lr": 1e-4,
    "min_lr": 1e-5,
    "lr_decay": 0.995,
    "weight_decay": 1e-3,
    "covg_threshold": 1e-2,
    "optimizer": "adam",
    "image_dataloader_type": "image_dataloader",
    "back": [
        {
            "type": "avgpool",
            "output": 3100
        },
        {
            "type": "fc",
            "input": 3100,
            "output": 128
        }
    ],
    "layers": [
        {
            "type": "conv2d",
            "kernel_size": 7,
            "stride": 2,
            "inplanes": 4,
            "outplanes": 32,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 2,
            "inplanes": 32,
            "outplanes": 64,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 1,
            "inplanes": 64,
            "outplanes": 32,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 2,
            "inplanes": 32,
            "outplanes": 8,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 1,
            "inplanes": 8,
            "outplanes": 4,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 1,
            "inplanes": 4,
            "outplanes": 2,
            "padding": 1
        },
        {
            "type": "avgpool",
            "output": 4096
        },
        {
            "type": "fc",
            "input": 4096,
            "output": 128
        },
        {
            "type": "fc",
            "input": 128,
            "output": 64
        },
        {
            "type": "end_fc",
            "input": 64
        }
    ],
    "model_output_dir": "../output/",
    "model_path": "",
    "save_model_iters": 10,
    "logging_iters": 1,
    "dropout": 0.0,
    "mode": "train",
    "data_loader_config": {
        "batch_size": 256,
        "enable_log_prediction": false,
        "data_dir": "D:\\SimpleClothSimulator\\data\\export_data\\500_5000_feature2_nonoise_500sample_real_4views_8_rot_gen",
        "train_perc": 0.8,
        "enable_data_augment": false,
        "enable_select_validation_set_inside": true,
        "enable_split_same_rot_image_into_one_set": true
    }
}