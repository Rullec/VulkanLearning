{
    "name": "CNNParamNet",
    "lr": 1e-4,
    "min_lr": 1e-5,
    "lr_decay": 0.995,
    "weight_decay": 3e-5,
    "covg_threshold": 1e-2,
    "optimizer": "adam",
    "image_dataloader_type": "image_dataloader",
    "back": [
        {
            "type": "avgpool",
            "output": 3100
        },
        {
            "type": "fc",
            "input": 3100,
            "output": 128
        }
    ],
    "layers": [
        {
            "type": "conv2d",
            "kernel_size": 7,
            "stride": 2,
            "inplanes": 4,
            "outplanes": 32,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 2,
            "inplanes": 32,
            "outplanes": 64,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 1,
            "inplanes": 64,
            "outplanes": 128,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 1,
            "inplanes": 128,
            "outplanes": 64,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 1,
            "inplanes": 64,
            "outplanes": 32,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 1,
            "inplanes": 32,
            "outplanes": 8,
            "padding": 1
        },
        {
            "type": "conv2d",
            "kernel_size": 3,
            "stride": 1,
            "inplanes": 8,
            "outplanes": 4,
            "padding": 1
        },
        {
            "type": "avgpool",
            "output": 8192
        },
        {
            "type": "fc",
            "input": 8192,
            "output": 512
        },
        {
            "type": "fc",
            "input": 512,
            "output": 128
        },
        {
            "type": "fc",
            "input": 128,
            "output": 64
        },
        {
            "type": "end_fc",
            "input": 64
        }
    ],
    "model_output_dir": "../output/",
    "model_path": "../output/07-06-14_25_08-0.1071.pkl",
    "save_model_iters": 10,
    "logging_iters": 1,
    "dropout": 0.0,
    "mode": "train",
    "data_loader_config": {
        "batch_size": 64,
        "enable_log_prediction": false,
        "data_dir": "../data/export_data/test_data",
        "data_dir.middle": "../data/export_data/uniform_sample10_noised8_2camnoised_2rot_4view",
        "data_dir.big": "../data/export_data/uniform_sample10_noised16_4camnoised_2rot_4view",
        "data_dir.small": "../data/export_data/test_data",
        "train_perc": 0.8,
        "enable_data_augment": true,
        "load_all_data_into_mem": true,
        "enable_test": false,
        "input_normalize_mode": "per_pixel"
    }
}